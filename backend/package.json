{
  "name": "kanaliiga-apex-backend",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "pretypeorm": "ts-node -r tsconfig-paths/register src/tools/write-ormconfig.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "posttypeorm": "ts-node -r tsconfig-paths/register src/tools/remove-ormconfig.ts",
    "genba": "node ./node_modules/bcryptjs/bin/bcrypt"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.14",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.14",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.14",
    "@nestjs/swagger": "^4.8.0",
    "@nestjs/typeorm": "^7.1.5",
    "@types/classnames": "^2.2.11",
    "@types/csurf": "^1.11.0",
    "@types/express": "^4.17.2",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.35",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.2.6",
    "csurf": "^1.11.0",
    "date-fns": "^2.19.0",
    "dotenv": "^8.2.0",
    "helmet": "^4.4.1",
    "mysql2": "^2.2.5",
    "nest-csv-parser": "^2.0.4",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.31"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/passport-http": "^0.3.8",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "prettier": "^1.18.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
